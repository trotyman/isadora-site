# Copie este arquivo para .env e preencha os valores

# Web3Forms
WEB3FORMS_ACCESS_KEY=COLOQUE_SUA_CHAVE_AQUI

# JWT Secret (gere um novo com: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=sua_chave_secreta_aqui

# Vercel KV (adicionado automaticamente ao conectar o KV no dashboard da Vercel)
# KV_URL=
# KV_REST_API_URL=
# KV_REST_API_TOKEN=
# KV_REST_API_READ_ONLY_TOKEN=

# Cloudflare R2 Storage
R2_ACCOUNT_ID=seu_account_id_aqui
R2_ACCESS_KEY_ID=seu_access_key_id_aqui
R2_SECRET_ACCESS_KEY=sua_secret_access_key_aqui
R2_BUCKET_NAME=isadora-uploads
R2_PUBLIC_URL=https://pub-xxx.r2.dev
